  "type": "module",
  "scripts": {
    "dev": "pnpm sync:version && vite",
    "sync:version": "node --input-type=module -e \"import fs from 'fs';const tauri=JSON.parse(fs.readFileSync('src-tauri/tauri.conf.json','utf8'));const pkgPath='package.json';const pkg=JSON.parse(fs.readFileSync(pkgPath,'utf8'));if(pkg.version!==tauri.version){pkg.version=tauri.version;fs.writeFileSync(pkgPath,JSON.stringify(pkg,null,2)+'\\n');}\"",
    "build": "pnpm sync:version && tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "altstore:source": "node scripts/generate-altstore-source.mjs",
    "altstore:publish": "powershell -ExecutionPolicy Bypass -File scripts/publish-altstore.ps1"
  },
